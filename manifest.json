{
  "manifest_version": 2,
  "name": "Context Search",
  "description": "Context Search allows you to add context to your google searches",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "chrome_url_overrides" : {
    "newtab": "popup.html"
  }
}
